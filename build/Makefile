# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  interpolation_config = debug

else ifeq ($(config),release)
  interpolation_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := interpolation

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

interpolation:
ifneq (,$(interpolation_config))
	@echo "==== Building interpolation ($(interpolation_config)) ===="
	@${MAKE} --no-print-directory -C . -f interpolation.make config=$(interpolation_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f interpolation.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   interpolation"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"